{% use 'entity/property_dockblock.php.twig' %}
{# @var property \Wame\SensioGeneratorBundle\MetaData\MetaProperty #}
{% block property %}
    {{- block('property_docblock') -}}
    {{- '\n    private $' ~property.name ~ ';\n' -}}
{% endblock %}


{% block property_methods %}

    {{ block('property_getter') }}
    {% if property.isId != true %}{{ block('property_setter') }}{% endif %}
{% endblock %}

{% block property_setter %}
    public function set{{ property.name | capitalize }}({{ property.nullable ? '?' : '' }}{{ property.returnType }} ${{ property.name }}): self
    {
        $this->{{ property.name }} = ${{ property.name }};

        return $this;
    }
{% endblock %}

{% block property_getter %}
    public function get{{ property.name | capitalize }}(): ?{{ property.returnType }}
    {
        return $this->{{ property.name }};
    }
{% endblock %}
