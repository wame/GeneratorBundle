    /**
     * @Route("/", name="{{ route_name_prefix }}_index")
     * @Method("GET")
     */
    public function indexAction({{ entity_class }}Datatable $datatable): Response
    {
        {%_ if use_voter is defined and use_voter u%}
            $this->denyAccessUnlessGranted({{ entity_class }}Voter::INDEX);

        {%_u endif %}
        $datatable->buildDatatable();

        return $this->render('{{ entity|tabalize|replace({'\\': '/'}) }}/index.html.twig', [
            'datatable' => $datatable,
        ]);
    }
