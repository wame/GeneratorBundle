    /**
     * @Route("/", name="{{ route_name_prefix }}_index")
     * @Method("GET")
     */
    public function indexAction(): Response
    {
        {% if use_voter is defined and use_voter u%}
            $this->denyAccessUnlessGranted({{ entity_class }}Voter::INDEX);

        {%u endif %}
        $em = $this->getDoctrine()->getManager();

        ${{ entity_pluralized }} = $em->getRepository({{ entity_class }}::class)->findAll();

        return $this->render('{{ twig_path_prefix }}/index.html.twig', [
            '{{ entity_pluralized }}' => ${{ entity_pluralized }},
        ]);
    }
